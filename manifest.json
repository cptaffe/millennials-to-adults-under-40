{
  "manifest_version": 2,
  "name": "MillennialsToAdultsUnder40",
  "version": "1.3",
  "description": "Replaces all instances of 'millennials' to 'adults under 40",
  "icons": {
    "48": "icons/millennials-48.png",
		"96": "icons/millennials-96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["millennials.js"]
    }
  ]
}

